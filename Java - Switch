// Switch
int a = 0;

switch (a) {
    
    case 0:
    a++; //action
    a++
    break;
    
    case 1:
    a--;
    break;
    
    case 3: case 4: case 5:
        System.out.println("was a 3, or a 4, or a 5");
        break;
    
default:
    a = a+5;     
}

